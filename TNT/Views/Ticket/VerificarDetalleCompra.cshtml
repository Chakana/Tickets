@model List<TNT.Controllers.TicketController.req_tickets_multiple>
<input id="new-ticket" type="hidden" value="@ViewBag.NewTickets">
<div class="row">
	<div class="col-md-4">Metodo de pago</div>
	<div class="col-md-4">@ViewBag.Metodo</div>
</div>
<div class="row">
	<div class="col-md-4">Nombre</div>
	<div class="col-md-4">@ViewBag.Nombre</div>
</div>
<div class="row">
	<div class="col-md-4">Nit</div>
	<div class="col-md-4">@ViewBag.Nit</div>
</div>
<div class="row">
	<div class="col-md-4">Email</div>
	<div class="col-md-4">@User.Identity.Name</div>
</div>
<div class="row">
	<div class="col-md-4">Evento</div>
	<div class="col-md-4">@ViewBag.Evento</div>
</div>
<table class="table col-md-12">
    <thead>
        <tr>
            <th>Cantidad</th>
            <th>Sector</th>
            <th>Precio Unitario</th>
        </tr>
    </thead>
    <tbody>
        @Html.EditorFor(m => m)
    </tbody>
</table>
<div class="row">
    <div class="col-md-4">Total</div>
    <div class="col-md-4">@ViewBag.Total Bs</div>
</div>
<div class="row">
    <div class="col-md-4">Comision</div>
    <div class="col-md-4">@ViewBag.Comision Bs</div>
</div>
<div class="row">
    <div class="col-md-4">Total a pagar</div>
    <div class="col-md-4">@ViewBag.PayTotal Bs</div>
</div>